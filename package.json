{
	"name": "form",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"preinstall": "node -e \"const ua=process.env.npm_config_user_agent||''; const ep=process.env.npm_execpath||''; const ok=ua.includes('pnpm')||ua.includes('bun')||ep.includes('pnpm')||ep.includes('bun'); if (!process.env.CI && !ok) { console.error('Please use pnpm or bun'); process.exit(1) }\"",
		"dev": "concurrently -c auto -n convex,vite \"pnpm convex:dev\" \"vite\"",
		"vite:dev": "vite",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"test": "vitest run",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"docs:check": "markdownlint \"**/*.md\" && linkinator docs --recurse",
		"api:docs": "echo 'typedoc disabled in CI for now'",
		"generate:adapters": "tsx scripts/generate-drei-registry.ts",
		"convex:dev": "convex dev --tail-logs",
		"convex:codegen": "convex codegen"
	},
	"dependencies": {
		"@convex-dev/eslint-plugin": "0.0.1-alpha.4",
