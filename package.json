		"convex-helpers": "^0.1.104",
		"d3": "^7.9.0",
		"lucide-react": "^0.536.0",
		"motion": "^12.23.12",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-resizable-panels": "^3.0.4",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11",
		"three": "^0.179.1",
		"zod": "^3.23.8"
